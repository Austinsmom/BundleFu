<?xml version="1.0" encoding="UTF-8"?>
<package packagerversion="1.8.0" version="2.0" xmlns="http://pear.php.net/dtd/package-2.0" xmlns:tasks="http://pear.php.net/dtd/tasks-1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://pear.php.net/dtd/tasks-1.0
    http://pear.php.net/dtd/tasks-1.0.xsd
    http://pear.php.net/dtd/package-2.0
    http://pear.php.net/dtd/package-2.0.xsd">
 <name>Du_BundleFu</name>
 <channel>pear.dotsunited.de</channel>
 <summary>Simple CSS and JavaScript bundling</summary>
 <description>Du\BundleFu is a PHP 5.3+ library which bundles multiple CSS and JavaScript files into a big package and sends it out at once.</description>
 <lead>
  <name>Jan Sorgalla</name>
  <user>jsor</user>
  <email>jan.sorgalla@dotsunited.de</email>
  <active>yes</active>
 </lead>
 <date>2010-12-02</date>
 <version>
  <release>1.0.0RC2</release>
  <api>1.0.0</api>
 </version>
 <stability>
  <release>beta</release>
  <api>stable</api>
 </stability>
 <license uri="https://github.com/dotsunited/du-bundlefu/blob/master/LICENSE">BSD License</license>
 <notes>https://github.com/dotsunited/du-bundlefu#readme</notes>
  <contents>
   <dir name="/">
     <file name="LICENSE" role="doc" />
     <file name="README.markdown" role="doc" />
     <dir name="src">
      <dir name="Du">
       <dir name="BundleFu">
        <dir name="Filter">
         <file install-as="Du/BundleFu/Filter/Callback.php" name="Callback.php" role="php" />
         <file install-as="Du/BundleFu/Filter/ClosureCompilerService.php" name="ClosureCompilerService.php" role="php" />
         <file install-as="Du/BundleFu/Filter/Filter.php" name="Filter.php" role="php" />
         <file install-as="Du/BundleFu/Filter/FilterChain.php" name="FilterChain.php" role="php" />
        </dir>
        <file install-as="Du/BundleFu/BundleFu.php" name="BundleFu.php" role="php" />
        <file install-as="Du/BundleFu/CssUrlRewriter.php" name="CssUrlRewriter.php" role="php" />
        <file install-as="Du/BundleFu/FileList.php" name="FileList.php" role="php" />
        <file install-as="Du/BundleFu/OutputHandler.php" name="OutputHandler.php" role="php" />
       </dir>
      </dir>
     </dir>
   </dir>
 </contents>
 <dependencies>
  <required>
   <php>
    <min>5.3.0</min>
   </php>
   <pearinstaller>
    <min>1.9.1</min>
   </pearinstaller>
  </required>
 </dependencies>
 <phprelease/>
</package>
